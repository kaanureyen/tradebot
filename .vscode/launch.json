{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug aggregator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/aggregator",
            "console": "integratedTerminal",
            "showLog": true,
            "env": {
                "GO111MODULE": "on"
            },
            "buildFlags": "-tags=debug",
            "output": "${workspaceFolder}/bin/aggregator"
        }
    ]
}